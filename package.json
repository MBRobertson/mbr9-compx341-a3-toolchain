{
  "name": "weather_report",
  "version": "0.0.1",
  "description": "Weather Report",
  "private": true,
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "body-parser": "*",
    "express": "*",
    "node-fetch": "^2.5.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "request": "^2.65.0"
  },
  "engines": {
    "node": "4.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-transform-regenerator": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "chai": "^3.4.0",
    "css-loader": "^2.1.1",
    "mocha": "^2.3.3",
    "node-sass": "^4.12.0",
    "nyc": "^14.1.1",
    "rewire": "^2.3.4",
    "sass-loader": "^7.1.0",
    "sinon": "^1.17.7",
    "style-loader": "^0.23.1",
    "supertest": "^1.1.0",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2"
  },
  "scripts": {
    "start": "node app.js",
    "test": "mocha --reporter=json > test/mochatest.json",
    "test-fvt": "mocha test-fvt --reporter=json > test-fvt/mochafvt.json",
    "test-cov": "nyc --reporter=json --reporter=json-summary --report-dir=test/coverage --temp-dir=test/.nyc_output npm run test",
    "build": "webpack --mode production",
    "watch": "webpack --watch --mode development",
    "clean-test": "rm -rf test/.nyc_output test/coverage test/mochatest.json",
    "clean-test-fvt": "rm -rf test-fvt/mochafvt.json",
    "clean-build": "rm -rf build/",
    "clean": "npm run clean-test && npm run clean-test-fvt && npm run clean-build"
  }
}
